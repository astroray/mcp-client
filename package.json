    "vue-json-pretty": "^2.4.0",
    "winston": "^3.17.0",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^35.1.2",
    "electron-builder": "^26.0.12",
    "concurrently": "^9.1.0",
    "@types/node": "^22.9.0",
    "vite": "^6.2.0",
